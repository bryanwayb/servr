{
    "hosts": {
        "*:80" : {
            "protocol": "http",
            "directories": {
                "/": {
                    "root": "../www/",
                    "options": "*",
                    "default": [ "index.jshtml", "index.html", "default.html" ]
                }
            },
            "cache": {
                "stat": true,
                "files": true,
                "handlers": true,
                "directoryDefaults": true,
                "urlMap": true 
            },
            "extensions": {
                "rewrite": [
                    {
                        "regex": "^rewrite\/test$",
                        "options": "i",
                        "replace": "",
                        "condition": "${USERAGENT}.indexOf('Trident') !== -1"
                    }
                ],
                "compression": {
                    "static": true,
                    "dynamic": true
                }
            }
        },
        "*:8080": "*:80"
    }
}